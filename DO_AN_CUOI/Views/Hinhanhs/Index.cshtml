@model IEnumerable<DO_AN_CUOI.Models.Hinhanh>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UrlHa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaddNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UrlHa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaddNavigation.Madd)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Mah">Edit</a> |
