@model DO_AN_CUOI.Models.Diemkhoihanh

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Diemkhoihanh</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <p>Nơi khởi hành</p>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Tendkh)
        </dd>
        <dt class="col-sm-2">
            <p>Địa chỉ</p>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dc)
        </dd>
        <dt class="col-sm-2">
            <p>Số điện thoại trạm khởi hành</p>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Sdt)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Madkh">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
